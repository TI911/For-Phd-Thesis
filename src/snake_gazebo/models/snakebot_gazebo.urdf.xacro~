<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">  

  <xacro:arg name="link_name" default="link"/>
  <xacro:property name="link_name" value="$(arg link_name)" />

  <xacro:macro name="snakebot_link">
    <gazebo reference="${link_name}${index}">
	  <selfCollide>true</selfCollide>
      <mu1 value="0.05" />
      <mu2 value="0.05" />
    </gazebo>
  </xacro:macro>

<!-- body units -->
<xacro:snakebot_link index="0"/>
<xacro:snakebot_link index="1"/>
<xacro:snakebot_link index="2"/>
<xacro:snakebot_link index="3"/>
